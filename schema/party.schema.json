{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Party list",
  "description": "All participating parties",
  "type": "array",
  "items": {
    "description": "Party",
    "type": "object",
    "properties": {
      "id": {
        "description": "Unique identifier",
        "type": "integer",
        "minimum": 0
      },
      "name": {
        "description": "Short party name",
        "type": "string"
      },
      "longname": {
        "description": "Full party name",
        "type": "string"
      }
    },
    "required": ["id", "name", "longname"]
  },
  "uniqueItems": true
}
